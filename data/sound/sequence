import sys
import random
from sugar.activity.activity import get_bundle_path
sys.path.append(get_bundle_path() + '/sound')
from sound import *

for i in range(25):
    pitch = random.randint(500,2000)
    amplitude = 5000
    duration = 0.1
    starttime = i * 0.1
    playSine(pitch, amplitude, duration, starttime)
audioOut()

